# GraphQL schema example
#
# https://gqlgen.com/getting-started/


type Book{
  id: ID!
  title: String!
  author: String!
  price: Float!
  image: String
}

type Query{
  books: [Book!]!
}

input NewBook{
  title: String!
  author: String!
  price: Float!
  image: String
}

type Mutation{
  createBook(input: NewBook!): Book!
}